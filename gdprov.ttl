	owl:inverseOf :isAnonymisedByStep ;
	rdfs:domain :DataAnonymisationStep ;
	rdfs:range :AnonymisedData ;
	rdfs:comment "Indicates that an DataAnonymisationStep transforms a Data object into AnonymisedData"@en ;
